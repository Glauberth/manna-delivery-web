generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model clientes {
  codcliente            Int           @id @default(autoincrement())
  codgrupo              Int
  grupo                 clientesgrupo @relation(fields: [codgrupo], references: [codgrupo])
  codhorarioatendimento Int?
  cnpj_cpf              String?       @db.VarChar(20)
  datacadastro          DateTime?     @db.DateTime(0)
  OK                    String?       @db.VarChar(255)
  razaosocial           String?       @db.VarChar(150)
  fantasia              String?       @db.VarChar(150)
  ipexterno             String?       @db.VarChar(15)
  dataatualizacaoip     DateTime?     @db.DateTime(0)
  bloqueado             String?       @db.VarChar(3)
  diasderenovacao       Int?
  vencimento            Int?
  checaratualizacao     String?       @db.VarChar(5)
  OBSERVACAO            String?       @db.VarChar(255)
  QTDMAXPC              Int?
  SITUACAO_CADASTRO     String?       @db.VarChar(50)
  EMAIL                 String?       @db.VarChar(255)
  VERSAO                String?       @db.VarChar(255)
  PROMESSA_DE_PAGAMENTO String?       @db.VarChar(3)
  DATA_INICIO_PROMESSA  DateTime?     @db.DateTime(0)
  DATA_LIMITE           DateTime?     @db.DateTime(0)
  TEMPOATENDIMENTO      String?       @db.VarChar(255)
  TIPOATENDIMENTO       String?       @db.VarChar(255)
  VersaoNFe             String?       @db.VarChar(255)
  VersaoNFCe            String?       @db.VarChar(255)
  SSLType               Int?
}

model clientesgrupo {
  codgrupo  Int        @id @default(autoincrement())
  nomegrupo String     @default("0") @db.VarChar(50)
  clientes  clientes[]
}

model clienteshorarioatendimento {
  codatendimento Int     @id @default(autoincrement())
  dhinicio       String? @db.VarChar(50)
  dhfim          String? @db.VarChar(50)
}

model produto {
  codproduto Int     @id @default(autoincrement())
  codgrupo   Int?
  codbarra   String?
  descricao  String?
  precovenda Float?
  observacao String?
  nome       String?
}

