generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model produto {
  codproduto Int     @id @default(autoincrement())
  codgrupo   Int?
  codbarra   String?
  descricao  String?
  precovenda Float?
  observacao String?
  nome       String?
}
