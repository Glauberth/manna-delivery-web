generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

//sempre que modificar o model, precisa executar esse comando:
//yarn prisma generate

model produto {
  codproduto Int     @id @default(autoincrement())
  codgrupo   Int?
  codbarra   String?
  descricao  String?
  precovenda Float?
  observacao String?
  nome       String?
}
